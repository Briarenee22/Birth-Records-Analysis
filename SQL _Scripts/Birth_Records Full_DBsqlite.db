BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS Birth_Record (
    Birth_ID INTEGER PRIMARY KEY,
    Date TEXT,
    Num_birth INTEGER,
    Hospital_ID TEXT,
    Doctor_ID INTEGER,
    Birth_type_ID INTEGER,
    FOREIGN KEY (Hospital_ID) REFERENCES Hospital(Hospital_ID),
    FOREIGN KEY (Doctor_ID) REFERENCES Doctor(Doctor_ID),
    FOREIGN KEY (Birth_type_ID) REFERENCES Birth_Type(Birth_type_ID)
);
CREATE TABLE IF NOT EXISTS Birth_Type (
    Birth_type_ID INTEGER PRIMARY KEY,
    Birth_type_name TEXT
);
CREATE TABLE IF NOT EXISTS Doctor (
    Doctor_ID INTEGER PRIMARY KEY,
    Name TEXT,
    Specialization TEXT
);
CREATE TABLE IF NOT EXISTS Hospital (
    Hospital_ID TEXT PRIMARY KEY,
    Name TEXT,
    Location TEXT
);
CREATE TABLE IF NOT EXISTS Mother (
	Mother_ID INTEGER PRIMARY KEY, 
	Age INTEGER,
	Health_Status TEXT,
	Region_ID INTEGER,
	FOREIGN KEY (Region_ID) REFERENCES Region (Region_ID)
);
CREATE TABLE IF NOT EXISTS Region (

	Region_ID INTEGER PRIMARY KEY,
	Name TEXT,
	State TEXT
);
